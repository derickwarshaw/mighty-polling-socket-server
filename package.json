{
  "author": "wosevision <jackson.teather@uoit.ca>",
  "name": "mighty-polling-socket-server",
  "description": "A batteries-included long-polling proxy microservice, built on Node/Express and Websockets with RxJS",
  "keywords": [
    "node",
    "websocket",
    "polling",
    "poll",
    "express",
    "rxjs",
    "http",
    "server",
    "service",
    "microservice"
  ],
  "version": "1.2.3",
  "repository": "wosevision/mighty-polling-socket-server",
  "main": "./server/polling-socket-server.js",
  "module": "./dist-esm/index",
  "jsnext:main": "./dist-esm/index.js",
  "types": "./dist/index",
  "typings": "./dist/index",
  "dependencies": {
    "@akanass/rx-http-request": "^2.6.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "rxjs": "^5.5.2",
    "xml2js": "^0.4.19"
  },
  "scripts": {
    "clean": "shx rm -rf _bundles dist dist-esm",
    "prebuild": "npm run clean",
    "build": "tsc -t es5 -m commonjs && tsc --outDir dist-esm && NODE_ENV=production webpack -p --progress",
    "test-client": "npm run test-server & webpack-dev-server",
    "test-server": "nodemon example/server --harmony",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags && npm publish"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "shx": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.9.1"
  }
}
